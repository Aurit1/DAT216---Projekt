<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root prefHeight="115.0" prefWidth="230.0" styleClass="background" stylesheets="@imat_receipt.css" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="dateLabel" alignment="CENTER" layoutY="21.0" prefHeight="17.0" prefWidth="230.0" text="Label" />
      <Button fx:id="openButton" layoutX="26.0" layoutY="82.0" mnemonicParsing="false" onAction="#openReceiptItemViewPane" text="Se Innehåll" />
      <Button fx:id="addButton" layoutX="150.0" layoutY="82.0" mnemonicParsing="false" onAction="#addButtonPressed" text="Lägg till" />
      <AnchorPane layoutX="-1.0" layoutY="43.0" prefHeight="23.0" prefWidth="230.0">
         <children>
            <Label alignment="CENTER_RIGHT" layoutX="16.0" layoutY="3.0" prefHeight="20.0" prefWidth="97.0" styleClass="amount_text" text="Pris:" />
            <Label fx:id="costLabel" layoutX="121.0" layoutY="5.0" prefHeight="17.0" prefWidth="109.0" text="Cost" />
         </children>
      </AnchorPane>
   </children>
</fx:root>
